                         <div class=\"md-layout-item md-size-100 text-right\">\r\n                                <button type=\"submit\" class=\"md-button md-success md-round mt-4 md-theme-default\">\r\n                                    <div class=\"md-ripple\">\r\n                                        <div class=\"md-button-content\" @click=\"rejectKyc()\" >Reject</div>\r\n                                    </div>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"md-layout-item md-size-100 text-center\">\r\n                                <div class=\"rejbtn\">\r\n                                    <button type=\"submit\" class=\"md-button md-success md-round mt-4 md-theme-default\">\r\n                                        <div class=\"md-ripple\">\r\n                                            <div class=\"md-button-content\" @click=\"approveKyc()\">Approve</div>\r\n                                         </div>\r\n                                    </button>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </md-card-content>\r\n            </md-card>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </form>\r\n    \r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n// const params = require('../_helpers/config.js');\r\n// const CONFIG = params.params; \r\n    \r\nexport default {  \r\n    data () {\r\n        return {\r\n            user: {\r\n                email: null,\r\n                accountType: null,\r\n                firstName: null,\r\n                lastName: null,\r\n                physicaladdress: null,\r\n                city: null,\r\n                country: null,\r\n                pincode: null,\r\n                status: null,\r\n                phoneNumber: null,\r\n                limit: null,\r\n       